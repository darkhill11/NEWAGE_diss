 168  vim(i,r) =  sum(g, vifm(i,g,r));
 169  evom(f,r) = sum(g, vfm(f,g,r));
 170  vb(r) = vom("c",r) + vom("g",r) + vom("i",r)
 171          - sum(f, evom(f,r))
 172          - sum(g,  vom(g,r)*rto(g,r))
 173          - sum(g,  sum(i, vdfm(i,g,r)*rtfd(i,g,r) + vifm(i,g,r)*rtfi(i,g,r)))
 174          - sum(g,  sum(f, vfm(f,g,r)*rtf(f,g,r)))
 175          - sum((i,s), rtms(i,s,r) *  (vxmd(i,s,r) * (1-rtxs(i,s,r)) + sum(j,vtwr(j,i,s,r))))
 176          + sum((i,s), rtxs(i,r,s) * vxmd(i,r,s));
