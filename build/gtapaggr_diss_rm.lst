 187  parameter       mprofit Zero profit for m,
 188                  yprofit Zero profit for y;
 189   
 190  mprofit(i,r) = vim(i,r) - sum(s, pvxmd(i,s,r)*vxmd(i,s,r)+sum(j, vtwr(j,i,s,r))*pvtwr(i,s,r));
 191  mprofit(i,r) = round(mprofit(i,r),5);
 192  display mprofit;
 193   
 194  yprofit(g,r) = vom(g,r)*(1-rto(g,r))
 195          - sum(i, vdfm(i,g,r)*(1+rtfd0(i,g,r))
 196                 + vifm(i,g,r)*(1+rtfi0(i,g,r)))
 197          - sum(f, vfm(f,g,r)*(1+rtf0(f,g,r)));
 198   
 199  yprofit(g,r) = round(yprofit(g,r),6)
 200  display yprofit;
