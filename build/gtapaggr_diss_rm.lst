 125  *       Declare some intermediate arrays which are required to
 126  *       evaluate tax rates:
 127   
 128  parameter       vdm(g,r)        Aggregate demand for domestic output,
 129                  vom(g,r)        Total supply at market prices;
 130   
 131  vdm(i,r) = sum(g, vdfm(i,g,r));
 132  vom(i,r) = vdm(i,r) + sum(s, vxmd(i,r,s)) + vst(i,r);
 133   
 134  parameter
 135          rtf0(f,g,r)     Primary factor and commodity rates taxes
 136          rtfd0(i,g,r)    Firms domestic tax rates
 137          rtfi0(i,g,r)    Firms  import tax rates
 138          rtxs0(i,r,s)    Export subsidy rates
 139          rtms0(i,r,s)    Import taxes rates;
 140   
 141  rtf0(f,g,r) = rtf(f,g,r);
 142  rtfd0(i,g,r) = rtfd(i,g,r);
 143  rtfi0(i,g,r) = rtfi(i,g,r);
 144  rtxs0(i,r,s) = rtxs(i,r,s);
 145  rtms0(i,r,s) = rtms(i,r,s);
 146   
 147  parameter       pvxmd(i,s,r)    Import price (power of benchmark tariff)
 148                  pvtwr(i,s,r)    Import price for transport services;
 149   
 150  pvxmd(i,s,r) = (1+rtms0(i,s,r)) * (1-rtxs0(i,s,r));
 151  pvtwr(i,s,r) = 1+rtms0(i,s,r);
 152   
 153  parameter
 154          vtw(j)          Aggregate international transportation services,
 155          vpm(r)          Aggregate private demand,
 156          vgm(r)          Aggregate public demand,
 157          vim(i,r)        Aggregate imports,
 158          evom(f,r)       Aggregate factor endowment at market prices,
 159          vb(*)           Current account balance;
 160   
 161  vtw(j) = sum(r, vst(j,r));
 162  vom("c",r) = sum(i, vdfm(i,"c",r)*(1+rtfd0(i,"c",r)) + vifm(i,"c",r)*(1+rtfi0(i,"c",r)))/(1-rto("c",r));
 163  vom("g",r) = sum(i, vdfm(i,"g",r)*(1+rtfd0(i,"g",r)) + vifm(i,"g",r)*(1+rtfi0(i,"g",r)))/(1-rto("g",r));
 164  vom("i",r) = sum(i, vdfm(i,"i",r)*(1+rtfd0(i,"i",r)) + vifm(i,"i",r)*(1+rtfi0(i,"i",r)))/(1-rto("i",r));
 165   
 166  vdm("c",r) = vom("c",r);
