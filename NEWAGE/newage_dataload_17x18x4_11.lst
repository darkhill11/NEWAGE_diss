 150          rtxs0(i,r,s)    Export subsidy rates
 151          rtms0(i,r,s)    Import taxes rates;
 152   
 153  rtf0(f,g,r) = rtf(f,g,r);
 154  rtfd0(i,g,r) = rtfd(i,g,r);
 155  rtfi0(i,g,r) = rtfi(i,g,r);
 156  rtxs0(i,r,s) = rtxs(i,r,s);
 157  rtms0(i,r,s) = rtms(i,r,s);
